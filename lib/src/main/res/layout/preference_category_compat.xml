<?xml version="1.0" encoding="utf-8"?>
<TextView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@android:id/title"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/category_margin_bottom"
    android:layout_marginEnd="@dimen/category_margin_end"
    android:layout_marginLeft="@dimen/category_margin_start"
    android:layout_marginRight="@dimen/category_margin_end"
    android:layout_marginStart="@dimen/category_margin_start"
    android:layout_marginTop="@dimen/category_margin_top"
